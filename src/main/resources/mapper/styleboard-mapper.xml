<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="StyleBoard">
	<select id="showStyleBoard" resultType="stydto">
		select * from styling
	</select>
	
	<select id="insertStyleBoard" parameterType="stydto">
		insert into styling values(styling_seq.nextval, #{styling_title}, #{styling_contents}, 0, sysdate , 0)
	</select>
	
	<select id="updateStyleBoard" parameterType="stydto">
		
	</select>
	
	<select id="deletStyleBoard" parameterType="stydto">
		delete from styling where styling_seq=#{styling_seq} 
	</select>
</mapper>
