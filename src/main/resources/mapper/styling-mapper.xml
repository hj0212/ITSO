<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Styling">

	<insert id="insertStylingVote" parameterType="svdto">
		insert into styling_vote values(styling_vote_seq.nextval, #{styling_title}, #{styling_contents},
		1,sysdate,#{styling_viewcount},#{styling_end},#{styling_endterm,jdbcType=DATE},#{styling_voternum,jdbcType=INTEGER},#{photo,jdbcType=VARCHAR})
	</insert>

</mapper>